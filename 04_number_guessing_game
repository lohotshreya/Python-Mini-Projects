import random

number = random.randint(1, 100)
guess = None
attempts = 0

print("ğŸ¯ Welcome to Number Guessing Game!")
print("Guess a number between 1 and 100")

while guess != number:
    guess = int(input("Enter your guess: "))
    attempts += 1

    if guess < number:
        print("Too low! ğŸ“‰")
    elif guess > number:
        print("Too high! ğŸ“ˆ")
    else:
        print(f"Correct! ğŸ‰ You guessed it in {attempts} attempts.")
